{"ast":null,"code":"import NavbarComponent from '@/components/common/NavbarComponent.vue';\nexport default {\n  name: 'App',\n  components: {\n    NavbarComponent\n  },\n  computed: {\n    mostrarNavbar() {\n      // Lista de rutas donde SÍ quieres mostrar la barra\n      const rutasConBarra = ['/dashboard', '/profile', '/grades', '/calendar', '/files', '/reports', '/preferences'];\n      return rutasConBarra.includes(this.$route.path);\n    }\n  }\n};","map":{"version":3,"names":["NavbarComponent","name","components","computed","mostrarNavbar","rutasConBarra","includes","$route","path"],"sources":["D:\\ECOCOBEDO FRONT\\CunaFrontend\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <NavbarComponent v-if=\"mostrarNavbar\" />\n    <router-view />\n  </div>\n</template>\n\n<script>\nimport NavbarComponent from '@/components/common/NavbarComponent.vue'\nexport default {\n  name: 'App',\n  components: {\n    NavbarComponent\n  },\n  computed: {\n    mostrarNavbar() {\n      // Lista de rutas donde SÍ quieres mostrar la barra\n      const rutasConBarra = [\n        '/dashboard',\n        '/profile',\n        '/grades',\n        '/calendar',\n        '/files',\n        '/reports',\n        '/preferences'\n      ]\n      return rutasConBarra.includes(this.$route.path)\n    }\n  }\n}\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  color: #2c3e50;\n  margin: 0;\n  padding: 0;\n}\n</style>\n"],"mappings":"AAQA,OAAOA,eAAc,MAAO,yCAAwC;AACpE,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,QAAQ,EAAE;IACRC,aAAaA,CAAA,EAAG;MACd;MACA,MAAMC,aAAY,GAAI,CACpB,YAAY,EACZ,UAAU,EACV,SAAS,EACT,WAAW,EACX,QAAQ,EACR,UAAU,EACV,cAAa,CACf;MACA,OAAOA,aAAa,CAACC,QAAQ,CAAC,IAAI,CAACC,MAAM,CAACC,IAAI;IAChD;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}